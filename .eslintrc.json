{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "import/no-unuresolved": 0,
    "import/extensions": 0,
    "indent": 0,
    "semi": [
      2,
      "always"
    ],
    "space-before-function-paren": 0
  }
}
